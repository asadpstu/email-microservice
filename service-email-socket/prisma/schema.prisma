generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Status {
  id         Int         @default(autoincrement()) @id
  receiver   String
  subject    String
  body       String
  createdAt  DateTime    @default(now())
  updatedAt  DateTime    @updatedAt
}
